{
    "name": "zofe/layout-module",
    "description": "layout module for rapyd-livewire application",
    "license": "mit",
    "type": "rapyd-module",
    "authors": [
        {
            "name": "Felice",
            "email": "felice.ostuni@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/config": "^11.0",
        "illuminate/contracts": "^11.0"
    },
    "config": {
        "allow-plugins": {
            "zofe/rapyd-module-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "App\\Modules\\Modules\\LayoutServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
